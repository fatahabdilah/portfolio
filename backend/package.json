{
  "name": "my-portfolio-backend",
  "version": "1.0.0",
  "description": "Backend API for the MERN Stack portfolio application, managing user authentication, skills, and projects with Cloudinary integration.",
  "main": "server.js",
  "scripts": {
    "start": "node -r dotenv/config server.js dotenv_config_path=.env.development",
    "dev": "nodemon -r dotenv/config server.js dotenv_config_path=.env.development",
    "vercel-build": "echo \"Building complete. Swagger assets loaded via CDN.\"",
    "seed": "node -r dotenv/config scripts/seed.js dotenv_config_path=.env.development",
    "test": "echo \"Error: No tests specified\" && exit 1"
  },
  "keywords": [
    "mern",
    "express",
    "mongoose",
    "jwt",
    "cloudinary",
    "portfolio"
  ],
  "author": "Fatah Abdilah",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.3",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}